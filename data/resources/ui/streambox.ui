<interface>
  <requires lib="gtk" version="4.0" />
  <requires lib="Adw" version="1.0" />
  <template class="PwStreamBox" parent="GtkWidget">
    <property name="layout-manager">
      <object class="GtkBinLayout" />
    </property>
    <style>
      <class name="streambox"/>
    </style>
    <child>
      <object class="PwVolumeBox" id="volumebox">
        <child type="extra">
          <object class="GtkBox">
            <property name="orientation">horizontal</property>
            <property name="spacing">6</property>
            <child>
              <object class="GtkLabel" id="onlabel">
                <property name="label" translatable="yes">on</property>
                <!-- <property name="visible" bind-source="outputdevice_dropdown"
                            bind-property="visible" bind-flags="sync-create" /> -->
              </object>
            </child>
            <child>
              <object class="PwStreamDropDown" id="output_dropdown" />
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
