<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
    <!-- interface-name channelbox.ui -->
    <requires lib="gtk" version="4.0" />
    <template class="PwChannelBox" parent="GtkListBoxRow">
        <child>
            <object class="GtkBox">
                <property name="spacing">6</property>
                <child>
                    <object class="GtkLabel" id="label">
                        <property name="label" bind-source="PwChannelBox"
                            bind-property="channelindex" bind-flags="bidirectional" />
                    </object>
                </child>
                <child>
                    <object class="GtkScale" id="scale">
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>

                        <property name="adjustment">
                            <object class="GtkAdjustment">
                                <property name="value" bind-source="PwChannelBox"
                                    bind-property="volume" bind-flags="bidirectional" />
                                <property name="lower">0</property>
                                <property name="upper">1</property>
                                <property name="step-increment">0.01</property>
                                <property name="page-increment">0.1</property>
                                <property name="page-size">0</property>
                            </object>
                        </property>

                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>